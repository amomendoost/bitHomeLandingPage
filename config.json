{
  "project": {
    "src_path": "src",
    "build_path": "dist",
    "root_file_name": "index.html"
  },
  "links_replacements": [
    
    {
      "target": "\"index.html\"",
      "replace_by": "\"/\""
    }
    
  ],
  "file_links_replacement": [
    {
      "target": "\"static/",
      "replace_by": "\"../static/"
    }
  ],
  "transforms": [
    {
      "name": "index",
      "filepath": "index.html",
      "move_to_subfolder": false,
      "apply_links_replacements": true,
      "apply_file_links_replacements": false
    }
  ]
}
